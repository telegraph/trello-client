{
  "source": "./src",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "accessor": {
          "access": [
            "public"
          ],
          "autoPrivate": true
        },
        "brand": {
          "logo": "./media/logo.png",
          "title": "Trello Client"
        }
      }
    }, {
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "all": true
      }
    }
  ]
}
