{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead"
      }
    ]
  ],
  "sourceMaps": "inline",
  "env": {
    "production": {
      "comments": false,
      "sourceMaps": false
    },
    "test": {
      "plugins": ["@babel/plugin-transform-runtime"],
    }
  }
}
